{
  "name": "tobai/magento2-geo-ip2",
  "description": "Integrate MaxMind GeoIP2 DB into Magento 2.",
  "keywords": ["magento2", "MaxMind"],
  "license":["MIT"],
  "type": "magento2-module",
  "version": "1.0.2",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": "~5.5.0|~5.6.0|~7.0.0",
    "magento/module-backend": "^100.1.0",
    "zendframework/zend-http": "~2.4.6",
    "monolog/monolog": "^1.16.0",
    "geoip2/geoip2": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "phpmd/phpmd": "^2.3",
    "squizlabs/php_codesniffer": "^2.5",
    "sebastian/phpcpd": "^2.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "files": ["registration.php"],
    "psr-4": {
      "Tobai\\GeoIp2\\": ""
    }
  },
  "authors": [
    {
      "name": "Yurii Torbyk",
      "homepage": "https://github.com/ytorbyk"
    }
  ]
}
